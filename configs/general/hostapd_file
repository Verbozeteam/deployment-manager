# Basic configuration

interface=wlan0
ssid={{SSID}}
channel={{CHANNEL}}
wmm_enabled=1

# WPA and WPA2 configuration

ht_capab=[HT40][SHORT-GI-20][DSSS_CCK-40]
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=1
wpa=2
wpa_passphrase={{WPA_PASS}}
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP
rsn_pairwise=CCMP

# Hardware configuration

driver=nl80211
ieee80211n=1
hw_mode=g
